# Postgres password
POSTGRES_PASSWORD=changeme_secure_password

# Pi-hole passwords (one per server, match the 'name' field in config.yml)
# Format: PIHOLE_PASSWORD_<NAME_IN_UPPERCASE>=password
PIHOLE_PASSWORD_PIHOLE1=your-pihole-password-here
PIHOLE_PASSWORD_PIHOLE2=your-pihole-password-here

# Telegram configuration
TELEGRAM_BOT_TOKEN=your-bot-token-here
TELEGRAM_CHAT_ID=your-chat-id-here

# Timezone for display (IANA format: America/Denver, Europe/London, etc.)
TZ=America/Denver

# CORS allowed origins (comma-separated)
# CORS_ORIGINS=http://localhost:3000,http://localhost:8000,https://yourdomain.com
